$TTL 5m ; 5 minutes
$ORIGIN example.org.
@       IN     SOA   ns.example.org. dnsadmin (
               2018050100 ; serial
               8h         ; refresh (8 hours)
               1h         ; retry (1 hour)
               1W         ; expire (1 week)
               1d         ; minimum (1 day)
               )
        IN     NS    ns.example.org.
        IN     NS    ns.example.net. ; out-of-zone

ns             A     192.168.1.2
dns            A     192.168.1.2
nexus          A     192.168.1.3
ntp            A     192.168.1.4
master         A     192.168.1.31
infra          A     192.168.1.32
app            A     192.168.1.33
